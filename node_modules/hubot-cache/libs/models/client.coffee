Model = require './model'

class Client extends Model
  id: null
  namespace: 'client'

  constructor: (@name) ->

  validation:
    'shortname':
      name: 'shortname'
      message: 'It should be a string between 10 and 14 characters long'
      validation: (data) ->
        data.should.be.a.string()

root = exports ? window
root.Client = Client